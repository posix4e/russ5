# Sample .env file for Fastlane Match configuration
# Copy this file to .env and fill in your values

# Apple Developer Team ID
TEAM_ID=ABCDE12345

# Apple ID for Developer Account
FASTLANE_APPLE_ID=your.email@example.com

# App-specific password for your Apple ID
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=abcd-efgh-ijkl-mnop

# Match repository URL
MATCH_GIT_URL=https://github.com/yourusername/russ5-certificates.git

# Password to encrypt/decrypt certificates in the Match repository
MATCH_PASSWORD=your-secure-password

# Bundle identifiers
APP_IDENTIFIER=xyz.russ.russ5
EXTENSION_IDENTIFIER=xyz.russ.russ5.Extension

# Optional: Provisioning profile specifiers (for manual signing)
PROVISIONING_PROFILE_SPECIFIER=match Development xyz.russ.russ5
EXTENSION_PROVISIONING_PROFILE_SPECIFIER=match Development xyz.russ.russ5.Extension

# BrowserStack credentials
BROWSERSTACK_USERNAME=your_browserstack_username
BROWSERSTACK_ACCESS_KEY=your_browserstack_access_key
# Optional: Use an existing app ID to skip the build and upload steps
# BROWSERSTACK_APP_ID=bs://your_app_id